public final class Operator {
	private String[] subject;

	public Operator(String[] subject) {
		super();
		this.subject = subject;
	}

	public String[] getSubject() {
		return subject;
	}

	public void setSubject(String[] subject) {
		this.subject = subject;
	}
	
	public Operator filter(String[] value, String search) {
		StringOperations newString = new StringOperations();
		subject = newString.filter(value, search);
	}
	public Operator removeDuplicates(String[] string){
		return null;
		
	}
	public Operator resize(String[] value, int length) {
		return null;
		
	}

}
